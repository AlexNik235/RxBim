<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <PlatformTarget>x64</PlatformTarget>
        <NoWarn>NU1701</NoWarn>
        <AssemblyName>$(MSBuildProjectName).$(ApiVersion)</AssemblyName>
        <UseWPF>true</UseWPF>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="CSharpFunctionalExtensions" Version="2.12.2"/>
        <PackageReference Include="BimLab.Revit.Api.2019" Version="1.0.1"/>
        <PackageReference Include="RevitTask" Version="3.0.0"/>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\PikTools.Di\PikTools.Di.csproj"/>
        <ProjectReference Include="..\PikTools.Shared.TableBuilder\PikTools.Shared.TableBuilder.csproj"/>
    </ItemGroup>

    <PropertyGroup>
        <Version>$(ApiVersion)</Version>
        <Company>pik-group.com</Company>
        <PackageIconUrl>https://favicon.pik.ru/favicon.ico</PackageIconUrl>
        <Authors>BimLab BimTeam</Authors>
        <Product>BimLab.PikTools</Product>
        <Copyright>Copyright ©2020 PIK Group</Copyright>
        <Description>PikTools Revit extensions Library</Description>
        <PackageTags>BIM BimLab</PackageTags>
        <PackageId>BimLab.PikTools.Shared.RevitExtensions</PackageId>
        <PackageProjectUrl>https://gitlab.com/pik_tools/shared/piktools.apiframework</PackageProjectUrl>
        <RepositoryUrl>https://gitlab.com/pik_tools/shared/piktools.apiframework.git</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
        <AssemblyAttribute Include="System.Windows.Markup.XmlnsPrefix">
            <_Parameter1>https://bimteam.ru/piktools/revit/abstractions</_Parameter1>
            <_Parameter2>ra</_Parameter2>
        </AssemblyAttribute>
        <AssemblyAttribute Include="System.Windows.Markup.XmlnsDefinition">
            <_Parameter1>https://bimteam.ru/piktools/revit/abstractions</_Parameter1>
            <_Parameter2>PikTools.Shared.RevitExtensions.Abstractions</_Parameter2>
        </AssemblyAttribute>
    </ItemGroup>

</Project>
