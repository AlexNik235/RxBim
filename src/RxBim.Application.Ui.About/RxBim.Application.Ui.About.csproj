<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

    <PropertyGroup>
        <TargetFramework>net472</TargetFramework>
        <PlatformTarget>x64</PlatformTarget>
        <NoWarn>NU1701</NoWarn>
        <UseWPF>true</UseWPF>
        <AssemblyName>$(MSBuildProjectName).$(ApiVersion)</AssemblyName>
    </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\RxBim.Di\RxBim.Di.csproj" />
    <ProjectReference Include="..\RxBim.Shared.Ui\RxBim.Shared.Ui.csproj" />
    <ProjectReference Include="..\RxBim.Shared\RxBim.Shared.csproj" />
  </ItemGroup>

  <PropertyGroup>
        <Version>$(ApiVersion)</Version>
         <Description>RxBim UI about box Library</Description>
        <PackageId>RxBim.Application.Ui.About</PackageId>
    </PropertyGroup>

</Project>
