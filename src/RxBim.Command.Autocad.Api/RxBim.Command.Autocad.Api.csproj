<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net47</TargetFramework>
        <PlatformTarget>x64</PlatformTarget>
        <NoWarn>NU1701</NoWarn>
        <AssemblyName>$(MSBuildProjectName).$(ApiVersion)</AssemblyName>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="AutoCAD2019.Base" Version="1.0.3" ExcludeAssets="build; runtime" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\RxBim.Command.Autocad.Generators\RxBim.Command.Autocad.Generators.csproj" />
        <ProjectReference Include="..\RxBim.Di\RxBim.Di.csproj" />
        <ProjectReference Include="..\RxBim.Shared\RxBim.Shared.csproj" />
    </ItemGroup>

    <PropertyGroup>
        <Version>$(ApiVersion)</Version>
         <Description>RxBim Command api Library for AutoCAD</Description>
        <PackageId>RxBim.Command.Autocad.Api</PackageId>
    </PropertyGroup>

</Project>
